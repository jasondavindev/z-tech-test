{
  "name": "z-tech-test",
  "version": "1.0.0",
  "main": "index.js",
  "author": "jasondavindev <jasondavin.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "format": "prettier --config ./.prettierrc --no-editorconfig --write \"{src,__tests__}/**/*.*(ts|json|yaml|yml)\"",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --fix",
    "formalint": "yarn format && yarn lint",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "run:migrations:dev": "yarn typeorm migration:run -c development",
    "run:migrations:test": "yarn typeorm migration:run -c test",
    "run:seeds": "NODE_ENV=development IS_SEED=1 yarn typeorm migration:run -c development",
    "create:migration": "yarn typeorm migration:create -d src/database/migrations/",
    "test": "NODE_ENV=test jest --maxWorkers=0"
  },
  "dependencies": {
    "class-transformer": "0.3.1",
    "class-validator": "0.8.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.2",
    "http-status-codes": "^1.4.0",
    "microframework-w3tec": "^0.6.3",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.7.7",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.9.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "merge": "^1.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1"
  }
}
